{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "esModuleInterop": true,
    "allowJs": true,
    "tsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "outDir": "./dist",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types"],
    "jsx": "react"
  },
  "include": ["src"]
}
